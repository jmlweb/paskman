{
  "name": "paskman",
  "version": "0.1.0",
  "repository": "git@github.com:jmlweb/paskman.git",
  "author": "<josemanuel@jmlweb.es>",
  "license": "MIT",
  "dependencies": {
    "localforage": "^1.5.0",
    "polished": "^1.8.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-rangeslider": "^2.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-persist": "^5.0.0-rc.6",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "shortid": "^2.2.8",
    "styled-components": "^2.2.1",
    "styled-map": "^2.0.1"
  },
  "devDependencies": {
    "cross-env": "^5.0.5",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "flow-bin": "^0.56.0",
    "lint-staged": "^4.2.3",
    "prettier": "^1.7.4",
    "raf": "^3.4.0",
    "react-scripts": "1.0.14",
    "react-snapshot": "^1.2.0",
    "react-test-renderer": "16",
    "redux-mock-store": "^1.3.0",
    "source-map-explorer": "^1.5.0"
  },
  "eslintConfig": {
    "extends": "./node_modules/react-scripts/config/eslint.js"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "start": "react-scripts start",
    "flow": "flow",
    "build": "cross-env NODE_ENV=production react-scripts build && react-snapshot",
    "test": "react-scripts test --env=jsdom --setupTestFrameworkScriptFile=raf/polyfill --collectCoverageFrom=src/**/*.js --collectCoverageFrom=!src/index.js --collectCoverageFrom=!src/registerServiceWorker.js",
    "analyze": "source-map-explorer build/static/js/main.*",
    "eject": "react-scripts eject"
  }
}
