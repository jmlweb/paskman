{
  "name": "paskman",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:jmlweb/paskman.git",
  "author": "<josemanuel@jmlweb.es>",
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "clean": "rimraf public",
    "build:webpack": "webpack --config webpack.config.prod.babel.js",
    "build:webpack:dev": "webpack --config webpack.config.dev.babel.js --watch",
    "build": "npm run clean && npm run build:webpack",
    "build:dev": "npm run clean && npm run build:webpack:dev",
    "start": "better-npm-run start",
    "server": "better-npm-run server",
    "test": "better-npm-run test",
    "test:e2e": "better-npm-run test:e2e",
    "lint": "eslint --cache .",
    "watch": "watch \"clear && npm run lint -s && npm run test -s\" source",
    "check": "npm run lint && npm run test && npm outdated --depth=0",
    "update": "updtr"
  },
  "betterScripts": {
    "start": {
      "command": "babel-node devServer.js | ./node_modules/bunyan/bin/bunyan",
      "env": {
        "NODE_PATH": "source"
      }
    },
    "server": {
      "command": "babel-node source/server/server.js | ./node_modules/bunyan/bin/bunyan",
      "env": {
        "NODE_PATH": "source"
      }
    },
    "test": {
      "command": "babel-node source/test/index.js",
      "env": {
        "NODE_PATH": "source"
      }
    },
    "test:e2e": {
      "command": "babel-node source/test/e2e.js",
      "env": {
        "NODE_PATH": "source",
        "NODE_PORT": 3000
      }
    }
  },
  "dependencies": {
    "bunyan-request-logger": "^1.1.0",
    "connect-cache-control": "^1.0.0",
    "express": "^4.14.0",
    "immutable": "^3.8.1",
    "moment": "^2.17.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "better-npm-run": "0.0.13",
    "browser-sync": "^2.18.5",
    "browser-sync-webpack-plugin": "^1.1.3",
    "cheerio": "^0.22.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.3",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "estraverse-fb": "^1.3.1",
    "file-loader": "^0.9.0",
    "image-webpack-loader": "^3.1.0",
    "nightwatch-autorun": "^3.1.0",
    "open": "^0.0.5",
    "react-transform-catch-errors": "^1.0.2",
    "redbox-react": "^1.3.3",
    "rimraf": "^2.5.4",
    "supertest": "^2.0.1",
    "tap-xunit": "^1.5.0",
    "tape": "^4.6.3",
    "updtr": "^0.2.3",
    "watch": "^1.0.1",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0"
  }
}
